<% form_for @contact_us do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :company %><br />
    <%= f.text_field :company %>
  </p>
  <p>
    <%= f.label :project %><br />
    <%= f.text_area :porject %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
